															<a data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fa fa-filter"></i> Address</a>
															{{ g.filter() }}	
														</div>
													</th>
													<th data-breakpoints="xs sm">
														<div class="dropdown">
															<a data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fa fa-filter"></i> Companies</a>
															{{ g.filter(false) }}	
														</div>
													</th>
													<th data-breakpoints="xs sm">
														<div class="dropdown">
															<a data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fa fa-filter"></i> Shops</a>
															{{ g.filter(false) }}	
														</div>
													</th>
													<th></th>
												</tr>
											</thead>
											<tbody>
